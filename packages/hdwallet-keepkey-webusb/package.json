    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@shapeshiftoss/hdwallet-core": "^0.18.0",
    "@shapeshiftoss/hdwallet-keepkey": "^0.18.0",
    "bytebuffer": "^5.0.1",
    "eventemitter2": "^5.0.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/w3c-web-usb": "^1.0.3",
    "microbundle": "^0.11.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  }
}
