{
  "name": "@shapeshiftoss/hdwallet-keepkey-webusb",
  "version": "0.18.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "scripts": {
    "build": "microbundle --target=web --name=keepkeyWebUSB",
    "clean": "rm -rf dist .rts2_*",
